{
  "name": "GlueApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn react-native run-android",
    "ios": "yarn react-native run-ios",
    "lint": "yarn eslint .",
    "start": "yarn react-native start",
    "test": "yarn jest",
    "clean": "rm -rf node_modules && yarn",
    "clean:ios": "cd ios && rm -rf Pods && rm -rf build && pod install && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:all": "yarn clean && yarn clean:ios && yarn clean:android",
    "reset": "yarn start --reset-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.0",
    "@react-native-community/netinfo": "^11.3.0",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.22",
    "@react-navigation/stack": "^6.3.20",
    "@tanstack/react-query": "^5.22.2",
    "axios": "^1.6.7",
    "i18next": "^23.11.0",
    "invariant": "^2.2.4",
    "react": "19.0.0",
    "react-i18next": "^14.0.5",
    "react-native": "0.78.2",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-keychain": "^8.2.0",
    "react-native-localize": "^3.0.6",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "^3.17.2",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-screens": "^3.37.0",
    "react-native-svg": "^15.11.2",
    "react-native-toast-message": "^2.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@bam.tech/react-native-make": "^3.0.3",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.2",
    "@react-native/eslint-config": "0.78.2",
    "@react-native/metro-config": "0.78.2",
    "@react-native/typescript-config": "0.78.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
